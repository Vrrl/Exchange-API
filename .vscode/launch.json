{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Serverless local Order Management",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "args": ["local:order-management"],
      "runtimeArgs": ["--inspect=9229"],
      "autoAttachChildProcesses": true,
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    },
    {
      "name": "Serverless local Account Management",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "args": ["local:account-management"],
      "runtimeArgs": ["--inspect=9230"],
      "autoAttachChildProcesses": true,
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    }
  ],
  "compounds": [
    {
      "name": "Serverless local All",
      "configurations": ["Serverless local Order Management", "Serverless local Account Management"],
      "stopAll": true
    }
  ]
}
